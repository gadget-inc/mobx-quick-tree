{
  "name": "@gadgetinc/mobx-quick-tree",
  "version": "0.3.3",
  "description": "A mirror of mobx-state-tree's API to construct fast, read-only instances that share all the same views",
  "source": "src/index.ts",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "github:gadget-inc/mobx-quick-tree",
  "license": "MIT",
  "files": [
    "README.md",
    "dist/src/**/*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "jest --runInBand",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:prettier": "prettier --check \"{spec,src}/**/*.{js,ts}\"",
    "lint:eslint": "eslint --quiet --ext ts,tsx spec src",
    "lint:fix": "eslint --ext ts --fix packages; prettier --write --check \"{spec,src}**/*.{js,ts}\"",
    "build": "rm -rf dist && tsc",
    "watch": "rm -rf dist && tsc --watch --preserveWatchOutput",
    "prepublishOnly": "yarn run build",
    "prerelease": "gitpkg publish"
  },
  "dependencies": {
    "lodash.memoize": "^4.1.2",
    "mobx": "^6.5.0",
    "mobx-state-tree": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@gadgetinc/eslint-config": "*",
    "@gadgetinc/prettier-config": "*",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "typescript": ">=4.6.3 <4.7.0"
  },
  "volta": {
    "node": "16.14.2"
  }
}
